{
  "name": "terraform-wrapper",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "apply:platform": "ts-node src/index.ts execute -o ../overview.yaml -e dev -c 'apply' -p platform/network/main/vpc,platform/network/main/subnet/public,platform/network/main/subnet/private,platform/eks-cluster/main",
    "destroy:platform": "ts-node src/index.ts execute -o ../overview.yaml -e dev -c 'destroy' -p platform/network/main/vpc,platform/network/main/subnet/public,platform/network/main/subnet/private,platform/eks-cluster/main"
  },
  "dependencies": {
    "yaml": "^1.10.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
